<h4>I am in the Views/Titles/Index.cshtml</h4>

@using Library.Models;
@model List<Library.Models.Title>;

<h1>All Titles</h1>

@if (@Model.Count == 0)
{
    <h3>There are currnetly NO Titles</h3>
}

<ol>
    @foreach (Title title in @Model)
    {
        <h1>@Html.ActionLink($"{title.TitleName}", "Details", new { id = title.TitleId })</h1>
    }   
</ol>

<h4>@Html.ActionLink("Go to Homepage", "Index", "Home")</h4>

